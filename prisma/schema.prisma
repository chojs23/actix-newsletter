// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "cargo prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Subscriptions {
  id            String   @id @default(cuid())
  email         String   @unique
  name          String
  subscribed_at DateTime @default(now())
}
